{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "module": "es2020",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@module/material": [
        "src/app/shared/external-modules/ng-material.module"
    ],
    "@module/core": [
      "src/app/core/index"
  ],
  "@module/app": [
    "src/app/app-server/index"
],
    "@module/serverside": [
      "src/app/serverside/index"
  ],
  "@module/shared": [
    "src/app/shared/index"
],
"@module/exam": [
  "src/app/exam/index"
],
"@module/quiz": [
  "src/app/quiz/index"
],
}
  }
}
